<UserControl
    x:Class="AutoDarkModeApp.UserControls.ColorPickerDialogContentControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:helpers="using:AutoDarkModeApp.Helpers"
    xmlns:local="using:AutoDarkModeApp.UserControls"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <StackPanel>
        <ColorPicker ColorChanged="SelectColorColorPicker_ColorChanged" Color="{x:Bind CustomColor}" />
    </StackPanel>

</UserControl>
